<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGrad5" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3498db;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2980b9;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="dryBagGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e74c3c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#c0392b;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="dryBagGrad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f39c12;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e67e22;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="dryBagGrad3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#27ae60;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#229954;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background - underwater scene -->
  <rect width="400" height="300" fill="url(#bgGrad5)"/>
  
  <!-- Water bubbles -->
  <circle cx="50" cy="80" r="3" fill="white" opacity="0.6"/>
  <circle cx="350" cy="60" r="2" fill="white" opacity="0.5"/>
  <circle cx="320" cy="90" r="4" fill="white" opacity="0.4"/>
  <circle cx="80" cy="120" r="2.5" fill="white" opacity="0.7"/>
  <circle cx="370" cy="120" r="3.5" fill="white" opacity="0.5"/>
  
  <!-- Large dry bag (main) -->
  <rect x="160" y="100" width="80" height="120" rx="8" fill="url(#dryBagGrad)"/>
  
  <!-- Roll-top closure -->
  <rect x="160" y="90" width="80" height="15" rx="8" fill="#a93226"/>
  <rect x="165" y="92" width="70" height="11" rx="5" fill="#922b21"/>
  
  <!-- Closure clips -->
  <rect x="170" y="88" width="8" height="6" rx="1" fill="#2c3e50"/>
  <rect x="185" y="88" width="8" height="6" rx="1" fill="#2c3e50"/>
  <rect x="207" y="88" width="8" height="6" rx="1" fill="#2c3e50"/>
  <rect x="222" y="88" width="8" height="6" rx="1" fill="#2c3e50"/>
  
  <!-- Transparent window -->
  <rect x="175" y="130" width="50" height="40" rx="5" fill="white" opacity="0.8" stroke="#bdc3c7" stroke-width="2"/>
  
  <!-- Phone inside visible through window -->
  <rect x="185" y="140" width="15" height="25" rx="2" fill="#2c3e50"/>
  <rect x="187" y="142" width="11" height="18" rx="1" fill="#3498db"/>
  <circle cx="192.5" cy="163" r="1.5" fill="#95a5a6"/>
  
  <!-- Wallet visible -->
  <rect x="205" y="145" width="20" height="12" rx="1" fill="#8b4513"/>
  
  <!-- Smaller dry bags -->
  <rect x="100" y="140" width="40" height="60" rx="5" fill="url(#dryBagGrad2)"/>
  <rect x="100" y="135" width="40" height="8" rx="5" fill="#d68910"/>
  
  <rect x="260" y="130" width="35" height="70" rx="4" fill="url(#dryBagGrad3)"/>
  <rect x="260" y="125" width="35" height="8" rx="4" fill="#1e8449"/>
  
  <!-- Carabiners/clips -->
  <ellipse cx="180" cy="85" rx="3" ry="5" fill="#95a5a6" stroke="#7f8c8d" stroke-width="1"/>
  <ellipse cx="220" cy="85" rx="3" ry="5" fill="#95a5a6" stroke="#7f8c8d" stroke-width="1"/>
  
  <!-- Smaller bag carabiners -->
  <ellipse cx="120" cy="132" rx="2" ry="3" fill="#95a5a6"/>
  <ellipse cx="277" cy="122" rx="2" ry="3" fill="#95a5a6"/>
  
  <!-- Waterproof seams -->
  <rect x="160" y="105" width="80" height="1" fill="#ffffff" opacity="0.8"/>
  <rect x="160" y="115" width="80" height="1" fill="#ffffff" opacity="0.8"/>
  <rect x="160" y="205" width="80" height="1" fill="#ffffff" opacity="0.8"/>
  
  <!-- Water resistance indicators -->
  <g opacity="0.7">
    <path d="M 140 160 Q 150 150 160 160" fill="none" stroke="#3498db" stroke-width="2"/>
    <path d="M 135 170 Q 145 160 155 170" fill="none" stroke="#3498db" stroke-width="2"/>
    <path d="M 130 180 Q 140 170 150 180" fill="none" stroke="#3498db" stroke-width="2"/>
  </g>
  
  <!-- Size labels -->
  <text x="120" y="210" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10" font-weight="bold">5L</text>
  <text x="200" y="235" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">20L</text>
  <text x="277" y="215" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10" font-weight="bold">10L</text>
  
  <!-- Submersion depth indicator -->
  <rect x="320" y="160" width="60" height="20" rx="10" fill="#2c3e50" opacity="0.8"/>
  <text x="350" y="172" text-anchor="middle" fill="#00ff00" font-family="Arial, sans-serif" font-size="10" font-weight="bold">IPX8</text>
  
  <!-- Text overlay -->
  <text x="200" y="270" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">DRY BAGS</text>
  <text x="200" y="285" text-anchor="middle" fill="#ecf0f1" font-family="Arial, sans-serif" font-size="12">Submersible Protection</text>
</svg>