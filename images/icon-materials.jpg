<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="materialGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#27ae60;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#229954;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="fabricGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#34495e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2c3e50;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="zipperGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f39c12;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e67e22;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="50" cy="50" r="45" fill="url(#materialGrad)"/>
  
  <!-- Fabric layers -->
  <g opacity="0.9">
    <!-- Base fabric layer -->
    <rect x="20" y="35" width="60" height="30" rx="3" fill="url(#fabricGrad)"/>
    
    <!-- Waterproof coating layer -->
    <rect x="22" y="37" width="56" height="26" rx="2" fill="#3498db" opacity="0.7"/>
    
    <!-- Top protective layer -->
    <rect x="24" y="39" width="52" height="22" rx="2" fill="#2c3e50" opacity="0.8"/>
  </g>
  
  <!-- Fabric weave pattern -->
  <g stroke="#95a5a6" stroke-width="0.5" opacity="0.6">
    <line x1="25" y1="40" x2="75" y2="40"/>
    <line x1="25" y1="43" x2="75" y2="43"/>
    <line x1="25" y1="46" x2="75" y2="46"/>
    <line x1="25" y1="49" x2="75" y2="49"/>
    <line x1="25" y1="52" x2="75" y2="52"/>
    <line x1="25" y1="55" x2="75" y2="55"/>
    <line x1="25" y1="58" x2="75" y2="58"/>
    
    <line x1="30" y1="35" x2="30" y2="65"/>
    <line x1="35" y1="35" x2="35" y2="65"/>
    <line x1="40" y1="35" x2="40" y2="65"/>
    <line x1="45" y1="35" x2="45" y2="65"/>
    <line x1="50" y1="35" x2="50" y2="65"/>
    <line x1="55" y1="35" x2="55" y2="65"/>
    <line x1="60" y1="35" x2="60" y2="65"/>
    <line x1="65" y1="35" x2="65" y2="65"/>
    <line x1="70" y1="35" x2="70" y2="65"/>
  </g>
  
  <!-- YKK Zipper -->
  <rect x="30" y="25" width="40" height="4" rx="2" fill="url(#zipperGrad)"/>
  <g fill="#d35400">
    <rect x="32" y="26" width="2" height="2"/>
    <rect x="36" y="26" width="2" height="2"/>
    <rect x="40" y="26" width="2" height="2"/>
    <rect x="44" y="26" width="2" height="2"/>
    <rect x="48" y="26" width="2" height="2"/>
    <rect x="52" y="26" width="2" height="2"/>
    <rect x="56" y="26" width="2" height="2"/>
    <rect x="60" y="26" width="2" height="2"/>
    <rect x="64" y="26" width="2" height="2"/>
    <rect x="66" y="26" width="2" height="2"/>
  </g>
  
  <!-- Zipper pull -->
  <rect x="68" y="24" width="4" height="8" rx="2" fill="#95a5a6"/>
  
  <!-- Reinforcement stitching -->
  <g stroke="#f1c40f" stroke-width="1" fill="none" stroke-dasharray="2,1">
    <rect x="22" y="37" width="56" height="26" rx="2"/>
    <line x1="25" y1="70" x2="75" y2="70"/>
    <line x1="25" y1="30" x2="75" y2="30"/>
  </g>
  
  <!-- Water droplets -->
  <g fill="#3498db" opacity="0.8">
    <circle cx="15" cy="20" r="2"/>
    <circle cx="85" cy="25" r="1.5"/>
    <circle cx="80" cy="75" r="2.5"/>
    <circle cx="20" cy="80" r="2"/>
  </g>
  
  <!-- Material certification badge -->
  <circle cx="75" cy="75" r="8" fill="#e74c3c"/>
  <text x="75" y="78" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="8" font-weight="bold">TPU</text>
  
  <!-- Quality indicator -->
  <polygon points="25,15 30,20 25,25 20,20" fill="#f1c40f"/>
  <text x="25" y="22" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="6" font-weight="bold">â˜…</text>
</svg>