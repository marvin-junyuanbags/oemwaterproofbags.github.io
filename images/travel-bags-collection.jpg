<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGrad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ecf0f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#bdc3c7;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="bagGrad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34495e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="handleGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#95a5a6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7f8c8d;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bgGrad2)"/>
  
  <!-- Airport/travel scene -->
  <rect x="0" y="250" width="400" height="50" fill="#34495e" opacity="0.3"/>
  <rect x="50" y="220" width="300" height="30" fill="#2c3e50" opacity="0.2"/>
  
  <!-- Main travel bag -->
  <rect x="150" y="120" width="100" height="70" rx="10" fill="url(#bagGrad2)"/>
  <rect x="155" y="125" width="90" height="60" rx="5" fill="#34495e" opacity="0.8"/>
  
  <!-- Bag details -->
  <rect x="160" y="135" width="80" height="3" fill="#e74c3c"/>
  <rect x="170" y="150" width="60" height="20" rx="3" fill="#2c3e50"/>
  <circle cx="185" cy="160" r="2" fill="#95a5a6"/>
  <circle cx="215" cy="160" r="2" fill="#95a5a6"/>
  
  <!-- Handle -->
  <rect x="190" y="110" width="20" height="8" rx="4" fill="url(#handleGrad)"/>
  <rect x="185" y="105" width="30" height="4" rx="2" fill="url(#handleGrad)"/>
  
  <!-- Wheels -->
  <circle cx="165" cy="195" r="8" fill="#2c3e50"/>
  <circle cx="235" cy="195" r="8" fill="#2c3e50"/>
  <circle cx="165" cy="195" r="5" fill="#95a5a6"/>
  <circle cx="235" cy="195" r="5" fill="#95a5a6"/>
  
  <!-- Telescopic handle -->
  <rect x="198" y="80" width="4" height="30" fill="#95a5a6"/>
  <rect x="196" y="78" width="8" height="4" rx="2" fill="#7f8c8d"/>
  
  <!-- Side pockets -->
  <ellipse cx="140" cy="155" rx="8" ry="15" fill="#2c3e50" opacity="0.7"/>
  <ellipse cx="260" cy="155" rx="8" ry="15" fill="#2c3e50" opacity="0.7"/>
  
  <!-- TSA lock -->
  <rect x="220" y="140" width="8" height="6" rx="1" fill="#e74c3c"/>
  <circle cx="224" cy="143" r="1.5" fill="#c0392b"/>
  
  <!-- Airplane silhouette in background -->
  <g opacity="0.2">
    <ellipse cx="320" cy="80" rx="25" ry="4" fill="#2c3e50"/>
    <rect x="310" y="78" width="20" height="4" fill="#2c3e50"/>
    <polygon points="295,80 310,78 310,82" fill="#2c3e50"/>
    <rect x="325" y="76" width="8" height="8" fill="#2c3e50"/>
  </g>
  
  <!-- Text overlay -->
  <text x="200" y="270" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="16" font-weight="bold">TRAVEL COLLECTION</text>
  <text x="200" y="285" text-anchor="middle" fill="#7f8c8d" font-family="Arial, sans-serif" font-size="12">Business & Leisure</text>
</svg>