<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGrad6" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#34495e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2c3e50;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="laptopBagGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a252f;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="laptopScreen" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3498db;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2980b9;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background - office/business environment -->
  <rect width="400" height="300" fill="url(#bgGrad6)"/>
  
  <!-- Office desk surface -->
  <rect x="0" y="250" width="400" height="50" fill="#95a5a6" opacity="0.3"/>
  
  <!-- Laptop bag main body -->
  <rect x="140" y="120" width="120" height="90" rx="8" fill="url(#laptopBagGrad)"/>
  <rect x="145" y="125" width="110" height="80" rx="5" fill="#2c3e50"/>
  
  <!-- Laptop compartment (slightly open) -->
  <rect x="150" y="130" width="100" height="70" rx="3" fill="#34495e" stroke="#95a5a6" stroke-width="1"/>
  
  <!-- Laptop visible inside -->
  <rect x="160" y="140" width="80" height="50" rx="2" fill="#95a5a6"/>
  <rect x="165" y="145" width="70" height="40" rx="1" fill="url(#laptopScreen)"/>
  
  <!-- Laptop keyboard area -->
  <rect x="165" y="175" width="70" height="10" rx="1" fill="#2c3e50"/>
  <g fill="#95a5a6">
    <rect x="170" y="177" width="3" height="2" rx="0.5"/>
    <rect x="175" y="177" width="3" height="2" rx="0.5"/>
    <rect x="180" y="177" width="3" height="2" rx="0.5"/>
    <rect x="185" y="177" width="3" height="2" rx="0.5"/>
    <rect x="190" y="177" width="3" height="2" rx="0.5"/>
    <rect x="195" y="177" width="3" height="2" rx="0.5"/>
    <rect x="200" y="177" width="3" height="2" rx="0.5"/>
    <rect x="205" y="177" width="3" height="2" rx="0.5"/>
    <rect x="210" y="177" width="3" height="2" rx="0.5"/>
    <rect x="215" y="177" width="3" height="2" rx="0.5"/>
    <rect x="220" y="177" width="3" height="2" rx="0.5"/>
    <rect x="225" y="177" width="3" height="2" rx="0.5"/>
  </g>
  
  <!-- Trackpad -->
  <rect x="195" y="181" width="15" height="8" rx="1" fill="#7f8c8d"/>
  
  <!-- Front pocket -->
  <rect x="150" y="105" width="100" height="20" rx="3" fill="#2c3e50" stroke="#95a5a6" stroke-width="1"/>
  
  <!-- Business cards/documents in front pocket -->
  <rect x="160" y="110" width="15" height="10" rx="1" fill="white" opacity="0.8"/>
  <rect x="180" y="108" width="20" height="12" rx="1" fill="#3498db" opacity="0.7"/>
  
  <!-- Handle -->
  <rect x="190" y="100" width="20" height="8" rx="4" fill="#7f8c8d"/>
  <ellipse cx="200" cy="95" rx="15" ry="4" fill="none" stroke="#7f8c8d" stroke-width="3"/>
  
  <!-- Shoulder strap -->
  <rect x="120" y="140" width="8" height="50" rx="4" fill="#7f8c8d"/>
  <rect x="272" y="140" width="8" height="50" rx="4" fill="#7f8c8d"/>
  
  <!-- Padded shoulder strap section -->
  <rect x="115" y="160" width="18" height="20" rx="9" fill="#34495e"/>
  <rect x="267" y="160" width="18" height="20" rx="9" fill="#34495e"/>
  
  <!-- Waterproof zippers -->
  <rect x="155" y="115" width="90" height="2" fill="#e67e22"/>
  <circle cx="160" cy="116" r="2" fill="#d35400"/>
  <rect x="155" y="205" width="90" height="2" fill="#e67e22"/>
  <circle cx="240" cy="206" r="2" fill="#d35400"/>
  
  <!-- RFID blocking indicator -->
  <rect x="220" y="135" width="20" height="15" rx="2" fill="#e74c3c" opacity="0.8"/>
  <text x="230" y="145" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="8" font-weight="bold">RFID</text>
  
  <!-- Shock absorption padding indicators -->
  <g opacity="0.6">
    <rect x="148" y="135" width="2" height="60" fill="#f39c12"/>
    <rect x="250" y="135" width="2" height="60" fill="#f39c12"/>
    <rect x="155" y="128" width="90" height="2" fill="#f39c12"/>
    <rect x="155" y="200" width="90" height="2" fill="#f39c12"/>
  </g>
  
  <!-- Security lock -->
  <rect x="175" y="110" width="6" height="8" rx="1" fill="#95a5a6"/>
  <circle cx="178" cy="114" r="1" fill="#2c3e50"/>
  
  <!-- Water droplets -->
  <circle cx="100" cy="80" r="2" fill="#3498db" opacity="0.8"/>
  <circle cx="300" cy="70" r="1.5" fill="#3498db" opacity="0.8"/>
  <circle cx="320" cy="90" r="2.5" fill="#3498db" opacity="0.8"/>
  
  <!-- Business environment elements -->
  <g opacity="0.3">
    <!-- Coffee cup -->
    <rect x="320" y="240" width="15" height="20" rx="2" fill="#8b4513"/>
    <rect x="322" y="235" width="11" height="8" rx="1" fill="#d2691e"/>
    
    <!-- Pen -->
    <rect x="300" y="245" width="2" height="15" fill="#2c3e50"/>
    <rect x="299" y="243" width="4" height="3" fill="#95a5a6"/>
  </g>
  
  <!-- Text overlay -->
  <text x="200" y="270" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">LAPTOP BAGS</text>
  <text x="200" y="285" text-anchor="middle" fill="#bdc3c7" font-family="Arial, sans-serif" font-size="12">Professional Protection</text>
</svg>